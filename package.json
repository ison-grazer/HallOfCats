{
  "name": "hallofcats",
  "version": "1.0.0",
  "description": "Gallery containing pictures of cats.",
  "main": "index.html",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "lite-server",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ison-grazer/HallOfCats.git"
  },
  "keywords": [
    "gallery",
    "flickr",
    "api"
  ],
  "author": "Isabella Fransson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ison-grazer/HallOfCats/issues"
  },
  "homepage": "https://github.com/ison-grazer/HallOfCats#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.13.9",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lite-server": "^2.6.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  }
}
